{
  "name": "framegraph",
  "displayName": "framegraph",
  "description": "A plugin for vscode to view your project's frame graph",
  "icon": "assets/framegraph.png",
  "version": "0.1.4",
  "license": "MIT",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.helloWorld",
    "onView:framegraphExplorer-DependencyTree"
  ],
  "repository": "https://github.com/sz-p/vscode-framegraph",
  "homepage": "https://github.com/sz-p/vscode-framegraph",
  "bugs": "https://github.com/sz-p/vscode-framegraph/issues",
  "main": "./outExtension/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "framegraph",
          "title": "%framegraph.framegraphTreeView%",
          "icon": "assets/framegraph.svg"
        }
      ]
    },
    "views": {
      "framegraph": [
        {
          "id": "framegraphExplorer-DependencyTree",
          "name": "%framegraph.DependencyTree%"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "framegraph.reOpenView",
          "group": "navigation@2"
        },
        {
          "command": "framegraph.refreshFile",
          "group": "navigation@1"
        }
      ]
    },
    "commands": [
      {
        "command": "framegraph.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "framegraph.refreshFile",
        "title": "%framegraph.refresh%",
        "icon": "$(refresh)"
      },
      {
        "command": "framegraph.createView",
        "title": "Open page"
      },
      {
        "command": "framegraph.reOpenView",
        "title": "%framegraph.openPage%",
        "icon": "$(browser)"
      },
      {
        "command": "framegraph.postMessage",
        "title": "post Message"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "%framegraph.title%",
      "properties": {
        "framegraph.entryFilePath": {
          "type": "string",
          "default": "",
          "description": "%framegraph.entryFilePath%"
        },
        "framegraph.folderPath": {
          "type": "string",
          "default": "",
          "description": "%framegraph.folderPath%"
        }
      }
    }
  },
  "scripts": {
    "watch": "npm-run-all -p watch:*",
    "build": "npm-run-all -p build:*",
    "vscode:prepublish": "npm-run-all -p build:*",
    "codeCount": "cloc ./ --exclude-dir=node_modules,out,outExtension,outWebView,assets,fileIcons",
    "compile": "tsc -p ./",
    "pretest": "npm-run-all -p build:*",
    "watch:Extension": "node ./scripts/watch.js",
    "watch:WebView": "node ./src/webView/scripts/watch.js",
    "build:Extension": "node ./scripts/build.js",
    "build:WebView": "node ./src/webView/scripts/build.js",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.22",
    "@types/vscode": "^1.41.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "css-loader": "^4.2.1",
    "glob": "^7.1.5",
    "html-webpack-plugin": "^4.3.0",
    "mocha": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2",
    "webpack": "^4.44.1"
  },
  "publisher": "sz-p",
  "dependencies": {
    "@fluentui/react": "^7.137.1",
    "@types/dependency-tree": "^7.2.0",
    "@types/lodash": "^4.14.165",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "ast-types": "^0.14.2",
    "babylon": "^6.18.0",
    "cssom": "^0.4.4",
    "d3": "^5.16.0",
    "dependency-tree": "^7.2.1",
    "file-loader": "^6.1.1",
    "get-dependency-tree": "^4.0.1",
    "lodash": "^4.17.20",
    "monaco-editor": "^0.21.2",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-monaco-editor": "^0.40.0",
    "react-redux": "^7.2.1",
    "react-resize-detector": "^5.2.0",
    "recast": "^0.20.4",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-promise": "^0.6.0",
    "string-random": "^0.1.3",
    "url-loader": "^4.1.1",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  }
}
